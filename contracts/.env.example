# Deployer key (required for forge script --broadcast)
PRIVATE_KEY=0x...

# Relayer address allowed to withdraw from TakeawayDeposit contracts (optional; defaults to deployer)
RELAYER_ADDRESS=0x...

# RPC URL for the chain you deploy to (e.g. Sepolia). Use with: --rpc-url $RPC_URL
RPC_URL=https://ethereum-sepolia-rpc.publicnode.com

# Filled by save-addresses script after deploy (used by frontend and relayer)
REGISTRY_ADDRESS=
FACTORY_ADDRESS=
